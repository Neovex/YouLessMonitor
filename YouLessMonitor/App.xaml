<Application x:Class="YouLessMonitor.App"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:local="clr-namespace:YouLessMonitor"
             xmlns:system="clr-namespace:System;assembly=mscorlib"
             StartupUri="MainWindow.xaml">
    <Application.Resources>
        <ResourceDictionary>
            <system:String x:Key="appName">YouLess Monitor</system:String>
            <BitmapImage x:Key="defaultIcon" UriSource="Images/icon.png"/>
            <BitmapImage x:Key="trayIcon" UriSource="Images/icon.ico"/>
            <BooleanToVisibilityConverter x:Key="BoolToVis" />
        </ResourceDictionary>
    </Application.Resources>
</Application>
